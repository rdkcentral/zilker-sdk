##
## template for a single POJO "destroy" function prototype
## when being removed from an icLinkedList
##

/*
 * 'linkedListItemFreeFunc' implementation for ${pojo_c_varname}
 */
void destroy_${pojo_c_varname}_fromList(void *item)
{
    if (item != NULL)
    {
        // call normal destroy function
        destroy_${pojo_c_varname}((${pojo_classname} *)item);
    }
}